{
  "manifest_version": 3,
  "name": "Hello Chrome Extension",
  "version": "2.0",
  "description": "A simple Chrome extension that displays 'Hello' when clicked.",
  "icons": {
    "16": "q.png",
    "48": "q.png",
    "128": "q.png"
  },
  "action": {   
    "default_icon": "q.png",
    "default_popup": "index_bootstrap.html"
  },
  "permissions": [
    "storage"
  ]
}
